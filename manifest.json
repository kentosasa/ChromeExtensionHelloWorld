{
  "manifest_version": 2,
  "name": "hello world",
  "version": "1.0",
  "description": "my first extension",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js":[
        "js/content.js"
      ],
      "matches":[
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
 "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "icon.png"
  }
}
